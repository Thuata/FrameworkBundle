{
    "name": "thuata/frameworkbundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Model Framwork bundle for symfony",
    "authors": [
        {
            "name": "Anthony Maudry",
            "email": "anthony.maudry@thuata.com",
            "homepage": "http://www.thuata.com/"
        }
    ],
    "autoload": {
        "psr-4": { "Thuata\\FrameworkBundle\\": "" }
    },
    "require": {
        "php": ">=5.5",
        "symfony/symfony": ">=2.7.*",
        "doctrine/orm": "~2.2,>=2.2.3,<2.5",
        "doctrine/dbal": "<2.5",
        "doctrine/doctrine-bundle": "~1.4",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~4.0",
        "sensio/framework-extra-bundle": "~3.0,>=3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "thuata/componentbundle" : "dev-master",
        "thuata/intercessionbundle" : "dev-master"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "master"
        }
    },
    "repositories" : [
        {
            "type" : "git",
            "url" : "git@github.com:Thuata/IntercessionBundle.git"
        },
        {
            "type" : "git",
            "url" : "git@github.com:Thuata/ComponentBundle.git"
        }
    ]
}
