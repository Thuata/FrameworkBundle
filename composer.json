{
    "name": "thuata/frameworkbundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Model Framwork bundle for symfony",
    "authors": [
        {
            "name": "Anthony Maudry",
            "email": "anthony.maudry@thuata.com",
            "homepage": "http://www.thuata.com/"
        }
    ],
    "autoload": {
        "psr-4": { "Thuata\\FrameworkBundle\\": "" }
    },
    "require": {
        "php": "~5.6",
        "symfony/symfony": "~3",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "thuata/componentbundle" : ">=1.0",
        "thuata/intercessionbundle" : ">=1.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3",
        "thuata/frameworktestbundle": ">=0.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "master"
        }
    },
    "repositories" : [
        {
            "type" : "git",
            "url" : "git@github.com:Thuata/IntercessionBundle.git"
        },
        {
            "type" : "git",
            "url" : "git@github.com:Thuata/ComponentBundle.git"
        }
    ]
}
